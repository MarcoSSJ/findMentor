<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.findmentorapp">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:targetApi="m">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.findmentorapp.LoginActivity"
            android:label="用户登录" >
        </activity>
        <activity
            android:name="com.example.findmentorapp.RegisterActivity"
            android:label="用户注册" >
        </activity>
        <activity
            android:name="com.example.findmentorapp.PersonalDataTActivity"
            android:label="用户资料" >
        </activity>
        <activity
            android:name="com.example.findmentorapp.PersonalDataSActivity"
            android:label="用户资料" >
        </activity>
        <activity
            android:name="com.example.findmentorapp.PersonalDataChangeTActivity"
            android:label="资料修改" >
        </activity>
        <activity
            android:name="com.example.findmentorapp.PersonalDataChangeSActivity"
            android:label="资料修改" >
        </activity>
        <activity
            android:name="com.example.findmentorapp.AccountActivity"
            android:label="账户管理" >
        </activity>
        <activity
            android:name="com.example.findmentorapp.InfoDetailActivity"
            android:label="详细信息" >
        </activity>
        <activity
            android:name="com.example.findmentorapp.FavoriteActivity"
            android:label="我的关注" >
        </activity>
        <activity
            android:name="com.example.findmentorapp.OthersDataActivity"
            android:label="他人资料" >
        </activity>
        <activity
            android:name="com.example.findmentorapp.ChangePasswordActivity"
            android:label="修改密码" >
        </activity>
    </application>

</manifest>